{
    "manifest_version": 3,
    "name": "Workday Schedule Exporter",
    "description": "Adds the functionality to export a course schedule from Workday",
    "version": "1.0.5",
    "author": "Gregory Conrad",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png"
    },
    "permissions": [
        "webRequest"
    ],
    "host_permissions": [
        "https://wd5.myworkday.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://wd5.myworkday.com/*"
            ],
            "js": [
                "main.js"
            ]
        }
    ],
    "background": {
        "service_worker": "worker.js"
    }
}