{
    "manifest_version": 2,
    "name": "Workday Schedule Exporter",
    "description": "Adds the functionality to export a course schedule from Workday",
    "version": "1.0",
    "permissions": [
        "webRequest",
        "https://wd5.myworkday.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://wd5.myworkday.com/*"
            ],
            "js": [
                "main.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "worker.js"
        ]
    }
}